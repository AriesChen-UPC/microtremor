{
    "workflow": "time-domain",
    "pre-processing": {
        "trim": {
            "apply": false,
            "begin": 0.0,
            "end": 1.0
        },
        "mute": {
            "apply": false,
            "method": "interactive",
            "window_kwargs": {}
        },
        "pad": {
            "apply": false,
            "df": 1.0
        }
    },
    "processing": {
        "transform": "fdbf",
        "fmin": 5,
        "fmax": 100,
        "vmin": 100,
        "vmax": 1000,
        "nvel": 200,
        "vspace": "linear",
        "fdbf-specific": {
            "weighting": "sqrt",
            "steering": "cylindrical"
        }
    },
    "signal-to-noise": {
        "perform": false,
        "noise": {
            "begin": -0.5,
            "end": 0.0
        },
        "signal": {
            "begin": 0.0,
            "end": 0.5
        },
        "pad": {
            "apply": true,
            "df": 1.0
        }
    }
}